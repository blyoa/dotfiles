Windows Registry Editor Version 5.00


[HKEY_CLASSES_ROOT\*\shell\gvim.exe]
@="Edit with Vim"
"Icon"="\"C:\\Program Files\\Vim\\gvim.exe\""

[HKEY_CLASSES_ROOT\*\shell\gvim.exe\command]
@="\"C:\\Program Files\\Vim\\gvim.exe\" \"%1\""

[HKEY_CLASSES_ROOT\Directory\background\shell\gvim.exe]
@="Open Vim here"
"Icon"="\"C:\\Program Files\\Vim\\gvim.exe\""

[HKEY_CLASSES_ROOT\Directory\background\shell\gvim.exe\command]
@="\"C:\\Program Files\\Vim\\gvim.exe\""

